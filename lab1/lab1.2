	AREA   seasea, CODE, READONLY
	EXPORT  __main	
__main   PROC
	ldr r1,=date1
	ldr r2,=date2
	adds r1,#8
	adds r2,#8
	movs r0,#0
	subs r0,#1
	
	ldr r3,[r1]
	ldr r4,[r2]
	cmp r3,r4
	bgt Here
	blt set_early
	
	subs r1,#8
	subs r2,#8
	ldr r3,[r1]
	ldr r4,[r2]
	cmp r3,r4
	bgt Here
	blt set_early
	
	adds r1,#4
	adds r2,#4
	ldr r3,[r1]
	ldr r4,[r2]
	cmp r3,r4
	bgt Here
	blt set_early
	

set_early
	movs r0,#1

Here	B	Here           ; Infinite loop to stop program execution
	ENDP
	align
	AREA dataof1d,DATA,readonly
date1 dcd 12,19,2013
date2 dcd 12,20,2013
	END                     ; End of assembly file
